<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <include file="struts-default.xml"/> 

	<package name="student" extends="struts-default">
		<action name="userlogin" class="student.UserLogin">
			<result name="STUDENT">studentIndex.jsp</result>
			<result name="TEACHER">teacherIndex.jsp</result>
			<result name="IDNOTEXIST">fail.jsp</result>
			<result name="ERRORPASSWORD">fail.jsp</result>
			<result name="Fail">fail.jsp</result>
			
		</action>
	</package>
</struts> 